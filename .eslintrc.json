{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
      "airbnb-base",
      "plugin:chai-friendly/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2022
    },
    "rules": {
      "func-names": ["error","never"],
      "max-len": [2, 150, 2, {
				"ignoreUrls": true
        }
      ],
      "new-cap": ["error", { "capIsNewExceptionPattern": "^express" }],
      "no-invalid-this": 0,
      "no-caller": 0,
      "no-console": 1,
      "guard-for-in": 0,
      "prefer-promise-reject-errors": 0,
      "no-prototype-builtins": 0,
      "object-curly-spacing": 1,
      "no-param-reassign": 0,
      "consistent-return": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": 0
    }
}
